{
  "name": "aws-s3-tools",
  "version": "0.0.1",
  "description": "A NodeJS module with AWS S3 utilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PedroS11/aws-s3-tools.git"
  },
  "keywords": [
    "aws",
    "s3",
    "typescript"
  ],
  "author": "Pedro Silva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PedroS11/aws-s3-tools/issues"
  },
  "homepage": "https://github.com/PedroS11/aws-s3-tools#readme",
  "scripts": {
    "build": "tsc",
    "test": "jest lib/.*.spec.ts",
    "test:coverage": "jest lib/.*.spec.ts --coverage",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "prettier": "prettier --write ./lib/**"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/eslint": "^7.2.9",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/prettier": "^2.2.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier && lint-staged && npm run test"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.ts": [
      "eslint --fix"
    ]
  },
  "nyc": {
    "include": [
      "lib"
    ],
    "all": true,
    "extension": [
      ".ts"
    ]
  }
}
